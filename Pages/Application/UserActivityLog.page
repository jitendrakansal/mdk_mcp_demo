{
	"ActionBar": {
		"Caption": "Activity Log",
		"PrefersLargeCaption": true,
		"_Type": "Control.Type.ActionBar"
	},
	"Controls": [
		{
			"_Type": "Control.Type.SectionedTable",
			"Sections": [
				{
					"Controls": [
						{
							"Caption": "Enable Logging",
							"IsEditable": true,
							"OnValueChange": "/MDK_MCP_Demo/Rules/Logging/ToggleLogging.js",
							"Value": false,
							"_Name": "EnableLogSwitch",
							"_Type": "Control.Type.FormCell.Switch"
						},
						{
							"AllowMultipleSelection": false,
							"Caption": "Log Level",
							"IsEditable": false,
							"IsSearchEnabled": false,
							"OnValueChange": "/MDK_MCP_Demo/Rules/Logging/SetUserLogLevel.js",
							"PickerItems": "/MDK_MCP_Demo/Rules/Logging/LogLevels.js",
							"_Name": "LogLevelListPicker",
							"_Type": "Control.Type.FormCell.ListPicker",
							"IsPickerDismissedOnSelection": true,
							"AllowEmptySelection": false
						}
					],
					"Visible": true,
					"_Type": "Section.Type.FormCell",
					"_Name": "FormCellSection0"
				},
				{
					"Controls": [
						{
							"OnPress": "/MDK_MCP_Demo/Actions/Logging/UploadLogProgress.action",
							"Alignment": "Center",
							"Title": "Send Activity Log",
							"_Name": "Send",
							"_Type": "Control.Type.FormCell.Button",
							"ButtonType": "Text",
							"Semantic": "Tint"
						}
					],
					"Visible": true,
					"_Type": "Section.Type.FormCell",
					"_Name": "FormCellSection1"
				}
			],
			"_Name": "SectionedTable0"
		}
	],
	"OnLoaded": "/MDK_MCP_Demo/Rules/Logging/UserLogSetting.js",
	"_Name": "UserActivityLog",
	"_Type": "Page"
}